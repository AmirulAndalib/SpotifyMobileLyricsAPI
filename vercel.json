{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "installCommand": "pnpm install",
    "buildCommand": "pnpm run build",
    "outputDirectory": "api",
    "functions": {
        "api/vercel.ts": {
            "runtime": "@vercel/node@3.2.14"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/vercel.ts"
        }
    ]
}